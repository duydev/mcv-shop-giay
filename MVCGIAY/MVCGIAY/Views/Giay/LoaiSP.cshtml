    {
        <div class="panel panel-default">
            <div class="panel-heading">
                <h4 class="panel-title">
                    <a href="@Url.Action("SPtheoloai", "GIAY", new { id = loaigiay.MaLoai })">@loaigiay.TenLoai</a>
                </h4>
            </div>
        </div>
     }
</div>
